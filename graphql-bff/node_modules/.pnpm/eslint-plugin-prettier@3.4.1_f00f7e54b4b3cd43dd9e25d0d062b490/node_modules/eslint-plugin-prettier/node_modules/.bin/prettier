#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/prettier@2.5.1/node_modules/prettier/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/prettier@2.5.1/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules:/Users/seion/self/graphql-bff-grpc-server/node_modules:/Users/seion/self/node_modules:/Users/seion/node_modules:/Users/node_modules:/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/eslint-plugin-prettier@3.4.1_f00f7e54b4b3cd43dd9e25d0d062b490/node_modules/eslint-plugin-prettier/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/eslint-plugin-prettier@3.4.1_f00f7e54b4b3cd43dd9e25d0d062b490/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/prettier@2.5.1/node_modules/prettier/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/prettier@2.5.1/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules:/Users/seion/self/graphql-bff-grpc-server/node_modules:/Users/seion/self/node_modules:/Users/seion/node_modules:/Users/node_modules:/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/eslint-plugin-prettier@3.4.1_f00f7e54b4b3cd43dd9e25d0d062b490/node_modules/eslint-plugin-prettier/node_modules:/Users/seion/self/graphql-bff-grpc-server/graphql-bff/node_modules/.pnpm/eslint-plugin-prettier@3.4.1_f00f7e54b4b3cd43dd9e25d0d062b490/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.5.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.5.1/node_modules/prettier/bin-prettier.js" "$@"
fi
